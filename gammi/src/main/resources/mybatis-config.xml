<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE configuration PUBLIC
"-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<setting name="cacheEnabled" value="false"/>
	</settings>

	<typeAliases>		
		<typeAlias type="com.andamiro.gammi.gammigroup.vo.GammiGroup" alias="GammiGroup" />
		<typeAlias type="com.andamiro.gammi.member.vo.Member" alias="Member" />
		<typeAlias type="com.andamiro.gammi.recipe.vo.Recipe" alias="Recipe" />
		<typeAlias type="com.andamiro.gammi.common.SearchDate" alias="SearchDate" />
		<typeAlias type="com.andamiro.gammi.common.Paging" alias="Paging" />
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/group-mapper.xml" />
		<mapper resource="mappers/member-mapper.xml" />
		<mapper resource="mappers/recipe-mapper.xml" />
	</mappers>
</configuration>
